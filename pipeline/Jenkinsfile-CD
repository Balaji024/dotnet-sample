@Library('shared-library') _
pipeline {
  agent any
   
   stages {       
    stage('Download Artifact Form Nexus') {
      steps {
        DownloadFromNexus()
      }
      }
    stage('Stop IIS') {
      steps {
        StopIIS()
      }
      }
    stage('DeployToIIS') {   
      steps{
        DeployToIIS()
       }
       }
     stage('Start IIS') {
      steps {
        StartIIS()
      }
      }
    }
}
