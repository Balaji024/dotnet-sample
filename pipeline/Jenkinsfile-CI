     //         }
        stage('Dotnet Build') {
           steps {
               BuildApp()               
           }
           }
      //  stage('Sonar Analysis') {
        //   steps {
          //     SonarApp(sonarlogin)               
           //}
        //}
        stage('Store war to Nexus') {
           steps {
               
               echo(buildid)
               StoreToNexus(buildid) 
               cleanWs()
           }
           }
        
    }
}  
