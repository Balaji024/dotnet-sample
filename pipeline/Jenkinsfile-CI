    agent any
    
    environment {
       sonarlogin = credentials('sonar_token')
       nexuslogin = credentials('nexus-login')
       buildid = env.BUILD_NUMBER
       }
    
    
    stages {
      
     //    stage('Install Dotnet Tools') {
     //         steps {
     //             InstallDotnetTools()               
